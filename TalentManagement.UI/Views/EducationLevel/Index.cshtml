@model IEnumerable<TalentManagement.Domain.Entities.EducationLevel>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}
<div class="container-xxl py-5">
    <div class="container">
        <h1 class="text-center mb-5 wow fadeInUp" data-wow-delay="0.1s">Education Levels</h1>
        <div id="tab-1" class="tab-pane fade show p-0 active">
            <a asp-controller="EducationLevel" asp-action="Create" class="btn btn-success float-end ">Add Education Level</a>
            <br />
            <br />
            @foreach (var item in Model)
            {
                <div class="job-item p-4 mb-4">
                    <div class="row g-4">

                        <div class="col-sm-12 col-md-8 d-flex align-items-center">

                            <div class="text-start ps-4">
                                <h5 class="mb-3">

                                    @Html.DisplayFor(modelItem => item.EducationLevelName)

                                </h5>



                            </div>
                        </div>


                    </div>
                </div>
            }
        </div>
    </div>
</div>
